{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true,
    "**/.dart_tool": true,
    "**/build": true,
    "**/ios/.gems": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "deno.enablePaths": ["./supabase/functions"],
  "cSpell.words": [
    "dotenv",
    "habitquokka",
    "lerp",
    "lilita",
    "Luca",
    "noto",
    "supabase"
  ]
}
