    {
      "name": "development (web)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "1337",
        "--dart-define-from-file",
        "development.json"
      ]
    }
  ]
}
